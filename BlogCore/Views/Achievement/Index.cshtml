@model IEnumerable<AchievementModel>

@{
    Layout = "_ManagementLayout";
    ViewData["Controller"] = "Achievements";
    ViewData["Title"] = $"{ViewData["Controller"]}";
    ViewData["Icon"] = "fas fa-trophy fa-fw";
}

<section id="achievement-management">
    <div class="container">
        <div class="row">
            <div class="col">
                <div class="management-header">
                    <h2 class="header">
                        Achievement Management
                    </h2>
                    <div class="underline"></div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <table-component v-bind:options="options"></table-component>
            </div>
        </div>
    </div>
</section>