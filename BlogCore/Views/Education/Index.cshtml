@model IEnumerable<EducationModel>

@{
    Layout = "_ManagementLayout";
    ViewData["Controller"] = "Education";
    ViewData["Title"] = $"{ViewData["Controller"]}";
    ViewData["Icon"] = "fas fa-university fa-fw";
}
<div class="management-header">
    <h2 class="header">
        Education Management
    </h2>
    <div class="underline"></div>
</div>
<div id="education-table">
    <table-component v-bind:url="endpoint" v-bind:ribbon="ribbon" :schema="schema"></table-component>
</div>
